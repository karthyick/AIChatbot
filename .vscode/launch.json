{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Uvicorn",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "app:app",
          "--reload"
        ],
        "env": {
          "HF_HUB_DISABLE_SSL": "1"
        },
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal"
      }
    ]
  }
  